<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.gugu.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_like"
        android:name="com.example.gugu.ui.like.LikeFragment"
        android:label="@string/title_like"
        tools:layout="@layout/fragment_like" />
    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.gugu.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/navigation_guide"
        android:name="com.example.gugu.ui.guide.GuideFragment"
        android:label="@string/title_guide"
        tools:layout="@layout/fragment_guide" />
    <fragment
        android:id="@+id/navigation_etc"
        android:name="com.example.gugu.ui.etc.EtcFragment"
        android:label="@string/title_etc"
        tools:layout="@layout/fragment_etc" />
</navigation>